# via jupyhai lab
include LICENSE
include *.md
include pyproject.toml
recursive-include jupyter-config *.json

include package.json
include install.json

graft jupyhai/labextension
graft jupyhai/nbextension
graft jupyhai/templates

prune **/node_modules
prune lib
prune binder

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints
